    InsertSQL.Strings = (
      'INSERT INTO TBFUNCIONARIO'
      '(CODIGO, NOME_COMPLETO, NOME_LIMPO, METAFONEMA, '
      '  SEXO, FOTO_3X4, CPF, RG_NUMERO, RG_ORGAO_EMISSOR, '
